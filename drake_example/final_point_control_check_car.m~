function xdot = car_approx_dynamics(t,x, xd)
    xdot = 0*x;
z = x(3);% - xd(3);
y = x(2);% - xd(2);
x = x(1);% - xd(1);

xdot(1) = ( - 2.3714395859512227 .* x + 0.56256024006965166 .* y + 0.10784937341160539 .* z + ((-7.5174560741032774 .* (z.^2)) ./ 2) + ((-1.7980406327180121 .* (z.^3)) ./ 6) + 3 .* ((-0.56256024006965166 .* y .* (z.^2)) ./ 6) + 2 .* ((-0.056444297215700823 .* y .* z) ./ 2) + 2 .* ((0.23793761322687251 .* x .* z) ./ 2) + 3 .* ((2.3714395859512227 .* x .* (z.^2)) ./ 6));

xdot(2) = ( - 0.23793761322687251 .* x + 0.056444297215700823 .* y + 7.4326636812211753 .* z + ((-7.6022484669853787 .* (z.^3)) ./ 6) + 2 .* ((-2.3714395859512227 .* x .* z) ./ 2) + 3 .* ((-0.056444297215700823 .* y .* (z.^2)) ./ 6) + 3 .* ((0.23793761322687251 .* x .* (z.^2)) ./ 6) + 2 .* ((0.56256024006965166 .* y .* z) ./ 2) + ((0.95294500306480867 .* (z.^2)) ./ 2));

xdot(3) = ( - 4.1923129192249178 .* x - 23.805345588087008 .* y - 27.544940116260324 .* z + ((-846.60461885970744 .* (z.^3)) ./ 6) + 2 .* ((-732.71984919665942 .* y .* (z.^2)) ./ 6) + ((-732.71984919665931 .* y .* (z.^2)) ./ 6) + ((-634.15008446546369 .* (y.^2) .* z) ./ 6) + 2 .* ((-634.15008446546358 .* (y.^2) .* z) ./ 6) + ((-548.83640370222508 .* (y.^3)) ./ 6) + 3 .* ((-144.54305906385079 .* x .* (z.^2)) ./ 6) + 6 .* ((-125.02904735057582 .* x .* y .* z) ./ 6) + ((-108.14835252421587 .* x .* (y.^2)) ./ 6) + 2 .* ((-108.14835252421585 .* x .* (y.^2)) ./ 6) + ((-92.098680083178436 .* (z.^2)) ./ 2) + 2 .* ((-79.297792095012682 .* y .* z) ./ 2) + ((-68.271815992024258 .* (y.^2)) ./ 2) + 2 .* ((-23.632887969554361 .* (x.^2) .* z) ./ 6) + ((-23.632887969554357 .* (x.^2) .* z) ./ 6) + 3 .* ((-20.424916679301397 .* (x.^2) .* y) ./ 6) + 2 .* ((-9.5885154665137886 .* x .* z) ./ 2) + 2 .* ((-8.1917868693513576 .* x .* y) ./ 2) + ((-3.6044973285438013 .* (x.^3)) ./ 6) + ((-0.04457948452076832 .* (x.^2)) ./ 2));

end

